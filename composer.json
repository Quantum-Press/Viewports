{
    "name": "qp/quantum-viewports",
    "type": "wordpress-plugin",
    "description": "Extend your BlockTheme to make standard block styles responsive!",
    "license": "non-public",
    "version": "0.9.9-dev",
    "autoload": {
        "psr-4": {
            "QP\\Viewports\\": "includes/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "yoast/wp-test-utils": "^1.2",
        "syde/phpcs": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=Syde includes/"
    }
}
